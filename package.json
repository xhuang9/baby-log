{
  "name": "baby-log",
  "version": "0.4.1",
  "author": "Parry Huang (https://github.com/xhuang9)",
  "engines": {
    "node": ">=22 <25"
  },
  "scripts": {
    "dev:spotlight": "npx @spotlightjs/spotlight",
    "dev:next": "next dev",
    "dev": "npm-run-all db-remote:neon db:migrate --parallel dev:*",
    "build:next": "next build --webpack",
    "build-local": "run-p db-server:memory build:next --race",
    "build": "run-s db:migrate build:next",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "clean": "rimraf .next out coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --noEmit --pretty",
    "typecheck": "tsc --noEmit --pretty",
    "check:deps": "knip",
    "check:i18n": "i18n-check -l src/locales -s en -u src -f next-intl",
    "commit": "commit",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "db-remote:neon": "npx get-db --yes --env .env.local --ref baby-log",
    "neon:claim": "npx get-db claim --env .env.local --ref baby-log",
    "db-server:file": "pglite-server --db=local.db --run 'npm run db:migrate'",
    "db-server:memory": "pglite-server --run 'npm run db:migrate'",
    "db:generate:named": "drizzle-kit generate --name",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "dotenv -c -- drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@arcjet/next": "^1.0.0",
    "@base-ui/react": "^1.1.0",
    "@clerk/localizations": "^3.35.3",
    "@clerk/nextjs": "^6.37.1",
    "@clerk/themes": "^2.4.51",
    "@logtape/logtape": "^1.3.7",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@sentry/nextjs": "^10.38.0",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@tanstack/react-query": "^5.90.20",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dexie": "^4.3.0",
    "dexie-react-hooks": "^4.2.0",
    "drizzle-orm": "^0.44.7",
    "import-in-the-middle": "^2.0.6",
    "input-otp": "^1.4.2",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.561.0",
    "next": "^16.1.6",
    "next-intl": "^4.8.1",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "pg": "^8.18.0",
    "posthog-js": "^1.336.4",
    "react": "^19.2.4",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.4",
    "recharts": "2.15.4",
    "require-in-the-middle": "^8.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@chromatic-com/playwright": "^0.12.8",
    "@clerk/types": "^4.101.14",
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@commitlint/prompt-cli": "^20.4.0",
    "@electric-sql/pglite-socket": "^0.0.19",
    "@eslint-react/eslint-plugin": "^2.8.1",
    "@lingual/i18n-check": "^0.8.19",
    "@next/bundle-analyzer": "^16.1.6",
    "@next/eslint-plugin-next": "^16.1.6",
    "@playwright/test": "^1.58.1",
    "@spotlightjs/spotlight": "4.5.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.9",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser": "^4.0.18",
    "@vitest/browser-playwright": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "babel-loader": "^10.0.0",
    "babel-plugin-react-compiler": "^1.0.0",
